{
  "name": "amazon-boats-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/server.ts",
    "dev:load-env": "dotenv -e .env.development",
    "dev:migrate:run": "npm run dev:load-env prisma migrate deploy",
    "dev:migrate:generate": "npm run dev:load-env prisma migrate dev",
    "test": "NODE_ENV=test npm run test:load-env vitest",
    "test:coverage": "NODE_ENV=test vitest --coverage",
    "test:load-env": "dotenv -e .env.test",
    "test:migrate:run": "npm run test:load-env prisma migrate deploy",
    "test:migrate:generate": "npm run test:load-env prisma migrate dev",
    "build": "tsup src",
    "start": "NODE_ENV=production node dist/server.js",
    "prod:load-env": "dotenv -e .env.production",
    "prod:migrate:generate": "npm run prod:load-env prisma migrate dev"
  },
  "prisma": {
    "seed": "npm run dev:load-env -- ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "bcrypt": "^5.1.0",
    "brazilian-values": "^0.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@vitest/coverage-c8": "^0.31.0",
    "dotenv-cli": "^7.2.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslist": "^1.0.0-beta.1",
    "prisma": "^4.14.0",
    "supertest": "^6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  }
}
